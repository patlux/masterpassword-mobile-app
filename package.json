    "react-native-web": "0.11.7",
    "react-native-webview": "7.4.3",
    "react-navigation": "4.0.10",
    "react-navigation-header-buttons": "3.0.4",
    "react-navigation-stack": "1.10.3",
    "scrypt-async": "2.0.1",
    "tslib": "1.10.0"

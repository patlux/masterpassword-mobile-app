{
  "name": "masterpassword-mobile-app",
  "version": "1.1.1",
  "author": "Patrick Wozniak <email@patwoz.de> (https://patwoz.de)",
  "homepage": "https://github.com/patlux/masterpassword-mobile-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/patlux/masterpassword-mobile-app.git"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": false,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .js,.ts,.tsx .",

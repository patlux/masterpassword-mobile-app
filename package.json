    "babel-preset-expo": "8.0.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-react-native": "3.8.1",
    "husky": "3.1.0",
    "is-ci-cli": "2.0.0",
    "jest": "24.9.0",
    "jest-expo": "36.0.1",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",

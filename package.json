    "test:ci": "jest",
    "test:watch": "jest --watch",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint test:ci",
    "commit": "npx git-cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@expo/browser-polyfill": "0.1.0",
    "crypto-js": "3.1.9-1",
    "expo": "36.0.2",
    "expo-constants": "8.0.0",
    "expo-crypto": "8.0.0",
    "expo-document-picker": "8.0.0",
    "expo-file-system": "8.0.0",
    "expo-font": "8.0.0",
    "expo-secure-store": "8.0.0",
    "raw.macro": "0.3.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",

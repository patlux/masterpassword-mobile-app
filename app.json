{
  "expo": {
    "name": "MPW",
    "slug": "mpw",
    "description": "A (offline) password manager mobile app based on mpw for android & iOS.",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#788968",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#788968"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.patwoz.mpw",
      "buildNumber": "1.2.0",
      "usesIcloudStorage": false,
      "usesAppleSignIn": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "de.patwoz.mpw",
      "versionCode": 360010200,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-transp.png",
        "backgroundColor": "#788968"
      },
      "permissions": []
    }
  }
}
